{
  "TokenOptions": {
    "Audience": "engin@engin.com", //Bu değer gereklidir.
    "Issuer": "engin@engin.com",   //Bunu uygulayan
    "AccessTokenExpiration": 10, //verilen token'ın geçerlilik süresini verir dakika cinsinden
    "SecurityKey": "mysupersecretkeymysupersecretkey" //Bu bizim decryption için kullandığımız anahtardır.
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
